{
  "name": "bouncing-logo",
  "displayName": "Bouncing DVD Logo",
  "description": "A bouncing DVD-style logo in a WebView panel",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:bouncingLogo.show"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bouncingLogo.show",
        "title": "Show Bouncing Logo",
        "category": "Bouncing Logo"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}